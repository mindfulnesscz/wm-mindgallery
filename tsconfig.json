{
  "files": [
    "./src/gutenberg/blocks/mindgallery-block/components/reactGallery.tsx"
  ],
  "exclude": [
    "/**/*.js"
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "esModuleInterop": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noEmit": false,
    "target": "ES6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "suppressImplicitAnyIndexErrors": true, // for lettings index of array be a string
  }
}